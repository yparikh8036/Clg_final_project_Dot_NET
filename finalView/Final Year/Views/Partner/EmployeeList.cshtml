 <div class="col-md-10">
        <div class="content-box-large">
            <div class="panel-heading">
                <div class="panel-title">EmployeeList</div>
                </div>
            <div class="panel-body">
                <table class="table">
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Name</th>
                            <th>Email</th>
                            <th>Mobile</th>
                            <th>Address</th>
                            <th>Username</th>
                            <th>Password</th>
                            <th>Active</th>
                            <th>Role</th>
                            <th>Photo</th>
                       
                        </tr>
                    </thead>
                    <tbody>
                        @for (int i = 0; i < Model.Rows.Count; i++)
                           {
						<tr>
							<td>@Model.Rows[i]["EmployeeID"]</td>
							<td>@Model.Rows[i]["Name"]</td>
							<td>@Model.Rows[i]["Email"]</td>
							<td>@Model.Rows[i]["Mobile"]</td>
							<td>@Model.Rows[i]["Address"]</td>
							<td>@Model.Rows[i]["Username"]</td>
							<td>@Model.Rows[i]["Password"]</td>
							<td>@Model.Rows[i]["IsActive"]</td>
							<td>@Model.Rows[i]["EmployeeRole"]</td>
							<td>@Model.Rows[i]["Photo"]</td>
							<td>
								<div class="row">
									<a class="glyphicon glyphicon-pencil" data-toggle="tooltip" title="Edit"href="/Partner/EmployeeEdit?Id=@Model.Rows[i]["EmployeeID"]">
									</a>
									<a class="glyphicon glyphicon-trash" data-toggle="tooltip" title="Delete" href="/Partner/EmployeeDelete?Id=@Model.Rows[i]["EmployeeID"]">
									</a>
								</div>
							</td>
						</tr>
                       }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
  